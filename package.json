{
  "name": "url_shortener",
  "version": "0.1.0",
  "scripts": {
    "watch": "babel-watch server.js",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn watch\" \"yarn client\"",
    "test": "cd client && yarn test",
    "install": "yarn install"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "lowdb": "^1.0.0",
    "mysql": "^2.15.0",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.8",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.js", "!<rootDir>/client/"]
  }
}
